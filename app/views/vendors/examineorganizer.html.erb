
<div class="container-fluid wow fadeInLeft animated">

	<div class="col-lg-3">
	<br>
		<div class="panel panel-default  panel-border-black">
			<div class="panel-heading">
				Events this organizer is also handling.
			</div>

			<div class="panel-body light-blue">
				<ul class="list-unstyled">
					<% @organizer.events.each do |event| %>
						<h4> <a href="<%= '/vendors/examineevent/' + event.id.to_s  %>" ><%= event.name  %></h4>
					<% end %>
					<li><button onclick="goBack()" class="btn btn-primary">Go Back</button></li>


				</ul>
				
			</div>
			
		</div>
	</div>
	
	<div class="col-lg-9">
	<br>
	
	</div>
	

	
</div>

<script>
function goBack() {
    window.history.back();
}
</script>